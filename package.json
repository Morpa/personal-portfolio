{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next:lint": "next lint",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "postinstall": "husky install",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@styled-icons/boxicons-logos": "^10.38.0",
    "@styled-icons/entypo": "^10.18.0",
    "@styled-icons/fa-solid": "^10.22.0",
    "@styled-icons/feather": "^10.18.0",
    "@styled-icons/material-outlined": "^10.18.0",
    "@styled-icons/zondicons": "^10.18.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.6.0",
    "graphql-request": "^3.5.0",
    "next": "^11.1.2",
    "next-pwa": "^5.0.0",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-awesome-reveal": "^3.5.1",
    "react-dom": "17.0.2",
    "react-icons": "^4.1.0",
    "react-masonry-css": "^1.0.14",
    "react-visibility-sensor": "^5.1.1",
    "styled-components": "5.3.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/add": "^3.0.0",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.5",
    "@next/eslint-plugin-next": "^11.1.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.26",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
